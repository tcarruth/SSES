
obj<-readRDS("C:/temp/Landscape.rda")
load(paste0("C:/Users/tcar_/Dropbox/Trout lakes/Data/BC regions/region_list"))

reg<-rep(NA,obj@nl)

for(rr in 1:length(obj@misc$region_names)){

  reg[(point.in.polygon(obj@lakex, obj@lakey, obj@misc$region_shapes[[rr]][,1],obj@misc$region_shapes[[rr]][,2])==1)]<-obj@misc$region_names[rr]

}

obj@misc$regs<-reg
obj@anam<-c("Generalists","Social","Enthusiasts","Occasional")

saveRDS(obj,"C:/temp/Landscape.rda")





